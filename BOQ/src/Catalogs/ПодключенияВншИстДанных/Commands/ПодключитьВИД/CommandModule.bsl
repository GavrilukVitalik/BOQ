Процедура ОбработкаКоманды ( ПараметрКоманды, ПараметрыВыполненияКоманды )

	ответ = МоделиBIM.ПодключитьКВншИстДанных ( ПараметрКоманды );
	Если ( ответ ) Тогда
		Сообщения.ПредупреждениеПоказать ( "ПодключениеКВншИстДанныхВыполнено" );
	Иначе
		Сообщения.ПредупреждениеПоказать ( "ПодключениеКВншИстДанныхНеВыполнено" );		
	КонецЕсли;

